      "source": [
        "mylist =  [3, 5, 7, 2, 8, 10, 11, 65, 72, 81, 99, 100, 150]\n",
        "n=len(mylist)\n",
        "moving_avg = []\n",
        "k=3\n",
        "\n",
        "for left in range(n-k+1):\n",
        "  right = left + k\n",
        "  sum_window = sum(mylist[left:right])\n",
        "  moving_avg.append(round(sum_window/k,3))\n",
        "\n",
        "print(moving_avg)"
      ],
